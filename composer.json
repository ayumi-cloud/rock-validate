{
    "name": "romeoz/rock-validate",
    "description": "Flexible validator for PHP with I18N.",
    "keywords": ["validate", "validation", "validator"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-validate/issues",
        "source": "https://github.com/romeOz/rock-validate"
    },

    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "0.10.*",
        "romeoz/rock-helpers" : "0.10.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "romeoz/rock-db" : "0.10.*"
    },
    "suggest": {
        "romeoz/rock-di": "required by module Rock Dependency Injection"
    },
    "autoload": {
        "psr-4": {
            "rock\\validate\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}