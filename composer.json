{
    "name": "romeoz/rock-validate",
    "description": "Flexible validator for PHP with I18N.",
    "keywords": ["validate", "validation", "validator"],
    "type": "library",
    "license": "MIT",
    "support": {
        "email": "serggalka@gmail.com",
        "issues": "https://github.com/romeOz/rock-validate/issues",
        "source": "https://github.com/romeOz/rock-validate"
    },

    "authors": [
        {
            "name": "Ruslan",
            "email": "serggalka@gmail.com"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/romeOz/rock-base"
    }],
    "require": {
        "php": ">=5.4.0",
        "romeoz/rock-base" : "*@dev",
        "romeoz/rock-helpers" : "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "rock\\validate\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}